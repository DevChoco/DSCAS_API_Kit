    "import os\n",
    "\n",
    "# 이미지 폴더 경로\n",
    "image_dir = r\"C:\\Users\\hj123\\OneDrive\\바탕 화면\\변환완료(원본)\" # 이미지 폴더 경로 (직접입력!!!)\n",
    "output_dir = r\"C:\\Users\\hj123\\OneDrive\\바탕 화면\" # 최종 .txt 저장 경로 (직접입력!!!)\n",
    "image_extensions = ['.jpg', '.jpeg', '.png']\n",
    "\n",
    "# 접두사 → 카테고리 맵핑 (dress -> d , shirt -> s) 새로운 의류 카테고리 생성 시 추가\n", 
    "# 폴더 종류 1. dresses(one piece, dress) 2. upper_body(shirt) 3. lower_body 이런 식으로 구동되니 사용시 수정 요함
    "category_map = {\n",
    "    'd': 'd',\n",
    "    'op': 'd',\n",
    "    's': 's'\n",
    "}\n",
    "\n",
    "# 결과 파일 핸들 저장용\n",
    "file_handles = {}\n",
    "\n",
    "try:\n",
    "    # 텍스트 파일 생성\n",
    "    for cat in category_map.values():\n",
    "        path = os.path.join(output_dir, f'test_pairs_unpaired_{cat}.txt')\n",
    "        file_handles[cat] = open(path, 'w', encoding='utf-8')\n",
    "\n",
    "    # 이미지 파일 순회\n",
    "    for filename in os.listdir(image_dir):\n",
    "        name, ext = os.path.splitext(filename)\n",
    "        if ext.lower() not in image_extensions:\n",
    "            continue\n",
    "\n",
    "        matched = False\n",
    "        for prefix, category in category_map.items():\n",
    "            # 정확히 prefix로 시작하는지 확인 (ex. 'op'만, 's'만 등)\n",
    "            if name[:len(prefix)] == prefix and (len(name) == len(prefix) or name[len(prefix)].isdigit()):\n",
    "                line = f\"_ {filename}\\n\"\n",
    "                file_handles[category].write(line)\n",
    "                print(f\"[{category}] {line.strip()}\")\n",
    "                matched = True\n",
    "                break\n",
    "\n",
    "        if not matched:\n",
    "            print(f\"❓ '{filename}'은 어떤 카테고리에도 맞지 않음 (건너뜀)\")\n",
    "\n",
    "finally:\n",
    "    # 파일 닫기\n",
    "    for f in file_handles.values():\n",
    "        f.close()\n",
    "\n",
    "print(\"\\n✅ 바탕화면에 정확한 접두사 기반 unpaired 텍스트 파일 생성 완료!\")"
